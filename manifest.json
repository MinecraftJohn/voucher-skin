{
  "manifest_version": 3,
  "name": "Wi-Fi Connect Voucher Skin",
  "version": "1.0.0",
  "description": "Load a custom voucher design on Omada Controller for Wi-Fi Connect.",
  "icons": {
    "16": "assets/img/favicon.png",
    "24": "assets/img/favicon.png",
    "32": "assets/img/favicon.png",
    "48": "assets/img/favicon.png",
    "128": "assets/img/favicon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://aps1-omada.tplinkcloud.com/omada/*/static/printVoucher.html?type=include&site=Default&token=*"
      ],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
